><tr
id=sl_svn13_284><td>            &#39;estimate_upload_size&#39;: function (file) {<br></td></tr
><tr
id=sl_svn13_285><td>                //no image scaling or zipped upload --&gt; return actual filesize<br></td></tr
><tr
id=sl_svn13_286><td>                if (JL.upload.params([&#39;zipDirectoriesOnAdd&#39;,&#39;uploadScaledImages&#39;]) === [false, false]){<br></td></tr
><tr
id=sl_svn13_287><td>                    return file.getLength();                <br></td></tr
><tr
id=sl_svn13_288><td>                }<br></td></tr
><tr
id=sl_svn13_289><td>                //zip compression is on --&gt; estimate compression ratio<br></td></tr
><tr
id=sl_svn13_290><td>                if (JL.upload.params(&#39;zipDirectoriesOnAdd&#39;) === true){<br></td></tr
><tr
id=sl_svn13_291><td>                    //calculate size by using estimated compression ratio<br></td></tr
><tr
id=sl_svn13_292><td>                    return file.getLength()*(1- JL.file.get_ratio(file));<br></td></tr
><tr
id=sl_svn13_293><td>                }<br></td></tr
><tr
id=sl_svn13_294><td>                //uploadScaledImages --&gt; estimate filesize of generated JPEGs<br></td></tr
><tr
id=sl_svn13_295><td>                if (JL.upload.params(&#39;uploadScaledImages&#39;) === true &amp;&amp; JL.file.is_image(file) === true){<br></td></tr
><tr
id=sl_svn13_296><td>                   var inst_dims = JL.upload.params(&#39;scaledInstanceDimensions&#39;).split(&#39;,&#39;);<br></td></tr
><tr
id=sl_svn13_297><td>                   var inst_qual = JL.upload.params(&#39;scaledInstanceQualityFactors&#39;).split(&#39;,&#39;);<br></td></tr
><tr
id=sl_svn13_298><td>                   var est_size = 0;<br></td></tr
><tr
id=sl_svn13_299><td>                   var dim_x, dim_y, qual;<br></td></tr
><tr
id=sl_svn13_300><td>                   for (num in inst_dims) {<br></td></tr
><tr
id=sl_svn13_301><td>                      [dim_x,dim_y] = inst_dims[num].split(&#39;x&#39;);<br></td></tr
><tr
id=sl_svn13_302><td>                      qual = inst_qual[num];<br></td></tr
><tr
id=sl_svn13_303><td>                      //very simple formula for estimating compressed JPEG filesize<br></td></tr
><tr
id=sl_svn13_304><td>                      //take 75% of the scaled InstanceDimensions area and multiply with a quality setting to the square<br></td></tr
><tr
id=sl_svn13_305><td>                      est_size += parseInt(dim_x)*parseInt(dim_y)*0.75*Math.pow(parseInt(qual)/1000,2);<br></td></tr
><tr
id=sl_svn13_306><td>                    } <br></td></tr
><tr
id=sl_svn13_307><td>                  return Math.round(est_size);<br></td></tr
><tr
